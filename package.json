{
  "name": "auditsec",
  "version": "1.0.0",
  "description": "E-commerce cybersécurité - vente de produits digitaux pour hackers éthiques",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:scan-worker": "nodemon backend/workers/scanWorker.js",
    "build": "cd frontend && npm run build",
    "start": "node backend/server.js",
    "install:all": "npm install && cd frontend && npm install",
    "setup": "npm run install:all",
    "seed": "node backend/scripts/seed.js"
  },
  "keywords": [
    "ecommerce",
    "cybersecurity",
    "pentest",
    "digital-products"
  ],
  "author": "AuditSec",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1498.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.4.5",
    "bullmq": "^5.58.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.0",
    "jose": "^5.2.4",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "openai": "^6.15.0",
    "openid-client": "^6.6.2",
    "pdf-parse": "^2.4.5",
    "pg": "^8.16.3",
    "stripe": "^14.7.0",
    "twilio": "^5.5.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
